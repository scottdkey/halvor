# PIA VPN Configuration
# Copy to .env and customize

# VPN container image
VPN_IMAGE=ghcr.io/scottdkey/pia-vpn:latest

# PIA credentials (required)
PIA_USERNAME=
PIA_PASSWORD=

# PIA server region (e.g., us_california, ca_montreal)
REGION=us_california

# Download fresh configs on startup
UPDATE_CONFIGS=true

# Enable debug logging
DEBUG=false

# Timezone
TZ=America/Chicago

# Path to VPN config files (auth.txt, .ovpn files)
VPN_CONFIG_PATH=/home/${USER}/config/vpn
